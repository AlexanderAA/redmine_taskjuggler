<% 
if issue.relations_to[0]
	i = 0
	depends = ""
	comma = ""
	issue.relations_to.each do |dep| 
		if dep.relation_type == "precedes" and @IssueFullName["t" + dep.issue_from_id.to_s]
			if i > 0 
				comma = ", "
			else
				i += 1
			end
			depends +=  comma + @IssueFullName["t" + dep.issue_from_id.to_s] 
		end
	end
	if depends > ""
%>				depends <%= depends %>
<%	end
end %>